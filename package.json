{
  "name": "vue3-form-validation",
  "version": "3.0.0",
  "description": "Form validation for Vue 3",
  "author": {
    "name": "jens",
    "email": "jens@doellmann.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JensDll/vue3-form-validation#readme"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "license": "MIT",
  "keywords": [
    "vue",
    "vuejs",
    "form",
    "validation",
    "utils",
    "vue-use"
  ],
  "scripts": {
    "build": "npx tsc --project ./main",
    "lint": "npx prettier --write .",
    "test": "jest --config ./main/jest.config.ts",
    "test-dts": "npx tsd",
    "test-all": "npm run build && npm run test && npm run test-dts",
    "dev": "vite"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.27",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@vitejs/plugin-vue": "^1.0.6",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-vue": "^7.4.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.1",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "tsd": "^0.14.0",
    "typescript": "^4.1.5",
    "vite": "^2.0.0-beta.69",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none",
    "endOfLine": "crlf"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx eslint --rule=\"no-console:error\" --fix-dry-run main"
    }
  },
  "tsd": {
    "directory": "test-dts"
  }
}
