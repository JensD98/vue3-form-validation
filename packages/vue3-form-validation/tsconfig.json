{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ES2020",
    "declaration": true,
    "lib": ["ES2020"],
    "downlevelIteration": true,
    "outDir": "dist",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["src/**/__tests__", "jest.config.ts", "test-dts"]
}
