{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ES2020",
    "declaration": true,
    "outDir": "../dist",
    "lib": ["ES2020"],
    "downlevelIteration": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["**/__tests__", "jest.config.ts", "test-dts"]
}
